<div id="new-album">
  <h1 class="reveal">Ajouter un nouveau swiper insta :</h1>

  <%= form_for @instagram do |f| %>
    <p>Images:</p>
    <%= f.file_field :images, as: :file, id:"file-input", multiple: "true" %>
        <br><br>
    <p>Videos:</p>
    <%= f.file_field :videos, as: :file, id:"file-input", multiple: "true" %>
    <br><br>
    <%= f.submit value: "AJOUTER LE SWIPPER INSTA", class:'btn', style:'border: none !important' %>
  <% end %>
  <br><br>

    <%= link_to destroy_user_session_path, method: :delete do %>
      <p><i class="fas fa-user" style="margin-right: 15px"></i>Log out</p>
    <% end %>

</div>

